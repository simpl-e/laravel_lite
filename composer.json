{
    "require": {
        "php": ">=7.0.0 <=7.0.23",
        "laravel/framework": "5.*",
        "tymon/jwt-auth": "dev-develop"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
